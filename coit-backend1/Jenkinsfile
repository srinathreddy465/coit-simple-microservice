pipeline{
    agent any
    stages{
        stage("Build"){
            steps{
                sh 'cd coit-backend1 && mvn install'
            }
        }
        
        
       stage("Deploy"){
            steps{
                sh 'ls -la'
                sh 'cd coit-backend1/target/ && java -jar sentiment-analysis-web-0.0.1.jar'
                
             
            }
        } 
        
    }
}
