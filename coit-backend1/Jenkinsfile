pipeline{
    agent any
    stages{
        stage("Build"){
            steps{
                sh 'cd coit-backend1 && mvn install'
            }
        }
        
        
            
            

       stage("Docker Build and Run"){
            steps{
                sh 'cd coit-backend1 && docker build -t coit-backend1 .'
                sh 'docker run -d -p 80:80 coit-backend1'
                
             
            }
        } 
        
    }
}
